{
  "expo": {
    "name": "The AI Council",
    "slug": "byok-the-ai-council",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "theaicouncil",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yaskovbs.theaicouncil",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.1059955046558-0s3mfdp6g427in9929jk7lanhltrsfd6",
              "theaicouncil"
            ]
          }
        ]
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.yaskovbs.theaicouncil",
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "the-hive-mind-robot.firebaseapp.com",
              "pathPrefix": "/__/auth/handler"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-splash-screen",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "EXPO_PUBLIC_SUPABASE_URL": "YOUR_SUPABASE_URL_HERE",
      "EXPO_PUBLIC_SUPABASE_ANON_KEY": "YOUR_SUPABASE_ANON_KEY_HERE",
      "EXPO_PUBLIC_GOOGLE_CLIENT_ID": "YOUR_GOOGLE_CLIENT_ID_HERE",
      "EXPO_PUBLIC_OPENROUTER_API_KEY": "YOUR_OPENROUTER_API_KEY_HERE",
      "EXPO_PUBLIC_SCRAPERAPI_API_KEY": "YOUR_SCRAPERAPI_API_KEY_HERE",
      "EXPO_PUBLIC_FIREBASE_API_KEY": "YOUR_FIREBASE_API_KEY_HERE",
      "EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN": "YOUR_FIREBASE_AUTH_DOMAIN_HERE",
      "EXPO_PUBLIC_FIREBASE_PROJECT_ID": "YOUR_FIREBASE_PROJECT_ID_HERE",
      "EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET": "YOUR_FIREBASE_STORAGE_BUCKET_HERE",
      "EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID": "YOUR_FIREBASE_MESSAGING_SENDER_ID_HERE",
      "EXPO_PUBLIC_FIREBASE_APP_ID": "YOUR_FIREBASE_APP_ID_HERE",
      "EXPO_PUBLIC_ADMOB_ANDROID_APP_ID": "YOUR_ADMOB_ANDROID_APP_ID_HERE",
      "EXPO_PUBLIC_ADMOB_IOS_APP_ID": "YOUR_ADMOB_IOS_APP_ID_HERE",
      "EXPO_PUBLIC_ADMOB_ANDROID_BANNER_ID": "YOUR_ADMOB_ANDROID_BANNER_ID_HERE",
      "EXPO_PUBLIC_ADMOB_IOS_BANNER_ID": "YOUR_ADMOB_IOS_BANNER_ID_HERE",
      "eas": {
        "projectId": "90c395c3-333a-4550-b8a8-cfcfa97dd97c"
      },
      "router": {}
    },
    "owner": "yaaaskovbs",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/90c395c3-333a-4550-b8a8-cfcfa97dd97c"
    }
  }
}
